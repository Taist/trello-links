icons =
  black:
    gear: ''
    remove: ''
    'trash-o': ''
    'chevron-down': ''
    'caret-down': ''

  gray40: #666666
    gear: ''
  white:
    remove: ''

get = (name, color) ->
  icons[color or 'black'][name] or ''

getDataImage = (name, color) ->
  'data:image/png;base64,' + get(name, color)

getURL = (name, color) ->
  'url("' + getDataImage(name, color) + '")'

module.exports = { get, getDataImage, getURL }
